<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>{{title}}</title>
	</head>
	<body class="body">
        <div id="container">
            <center><h1>{{title}}</h1></center>
        </div>
            <table align="center" width="70%" border="2" >
                <tr>
                    <th>Variable</th>
                    <th>Usuario</th>
                    <th>Cantidad de Registros</th>
                    <th>Periodo</th>
                    <th>Fecha</th>
                </tr>
                {{#reportes}}
                <tr>
                    <td>{{Variable}}</td>
                    <td>{{Usuario}}</td>
                    <td>{{RegistrosReportados}}</td>
                    <td>{{CodigoPeriodo}}</td>
                    <td>{{FechaReporte}}</td>
                </tr>
                {{/reportes}}
            </table>
    </body>
</html>
